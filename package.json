{
  "name": "wavr",
  "description": "Web audio visualizer",
  "scripts": {
    "start": "budo src/index.js -ol -- -t [ babelify --presets [ es2015 stage-2 ] ] | garnish",
    "html": "pug -o build src/index.pug",
    "build:js": "browserify src/index.js -t [ babelify --presets [ es2015 stage-2 ] ] -p licensify | uglifyjs -m -c warnings=false --comments '/This header is generated by licensify/' -o build/bundle.js",
    "rm": "rm -rf build/**",
    "cp": "cp -r sounds build/sounds",
    "test": "echo \"Error: no test specified\"",
    "build": "npm run rm && npm run html && npm run build:js && npm run cp",
    "deploy": "gh-pages -d build -xt --repo https://$GH_TOKEN@github.com/sugarshin/wavr.git"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/sugarshin/wavr"
  },
  "author": "sugarshin",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "insert-stylesheet": "~0.1.0",
    "raf-loop": "^1.1.3",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "budo": "^8.3.0",
    "garnish": "^5.2.0",
    "gh-pages": "~0.11.0",
    "licensify": "^3.1.2",
    "pug-cli": "1.0.0-alpha6",
    "uglify-js": "^2.7.0"
  }
}
